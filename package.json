{
	"name": "website-yagasaki",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"vercel": "next build",
		"start": "next start",
		"lint": "next lint",
		"generate-rss": "bun src/scripts/gen-rss.ts",
		"create:mylint": "bunx --bun biome init && bun mylint",
		"mylint": "bunx biome format --write",
		"commit": "git pull && bun run vercel && bun mylint && read -p 'Descriptions Commit: ' message && git add . && echo -n \"$message - \" && node getTime.js | xargs -I {} git commit -m \"$message - {}\" && git push",
		"clean": "rm -rf node_modules && bun install",
		"prebuild": "bun run generate-rss"
	},
	"dependencies": {
		"@icons-pack/react-simple-icons": "^9.5.0",
		"@vercel/analytics": "^1.5.0",
		"dotenv": "^16.4.5",
		"feed": "^5.1.0",
		"gray-matter": "^4.0.3",
		"lucide-react": "^0.395.0",
		"moment": "^2.30.1",
		"next": "14.2.32",
		"next-seo": "^6.8.0",
		"react": "^18",
		"react-dom": "^18",
		"remark": "^15.0.1",
		"remark-html": "^16.0.1",
		"rss": "^1.2.2",
		"sugar-high": "^0.7.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.1",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.0.4",
		"postcss": "^8",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}
