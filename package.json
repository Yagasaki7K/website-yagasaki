{
	"name": "website-yagasaki",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"vercel": "next build",
		"start": "next start",
		"lint": "next lint",
		"generate-rss": "bun src/scripts/gen-rss.ts",
		"commit": "bun upgrade && git pull && bun update && bun run vercel && read -p 'Descriptions Commit: ' message && git add . && echo -n \"$message - \" && node getTime.js | xargs -I {} git commit -m \"$message - {}\" && git push",
		"clean": "rm -rf node_modules && bun install",
		"prebuild": "bun run generate-rss"
	},
	"dependencies": {
		"@icons-pack/react-simple-icons": "^9.7.0",
		"@vercel/analytics": "^1.5.0",
		"dotenv": "^16.6.1",
