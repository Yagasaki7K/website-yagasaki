		"framer-motion": "^12.23.24",
		"gray-matter": "^4.0.3",
		"lucide-react": "^0.395.0",
		"moment": "^2.30.1",
		"next": "^16.0.1",
		"next-seo": "^6.8.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"remark": "^15.0.1",
		"remark-html": "^16.0.1",
		"rss": "^1.2.2",
